<%= link_to 'Настройки модели', :controller => :model, :model => @model_name, :action => :index %>

<table>
	<tr>
		<th>№</th>
		<% colzer(@model.classify.constantize.new).each do |f| %>
			<th><%= f.label %></th>
		<% end %>
		<th></th>
		
		
	</tr>
	
</table>

<ul class="index_list">
	<% @objects.each_index do |index| %>
		<li><%= @objects[index].render_in_list %></li>
	<% end %>
</ul>